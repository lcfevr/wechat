#order-index .navbar {
  position: fixed;
  top: 0;
  width: 100%;
  display: flex;
  z-index: 20;
  height: 50px;
  line-height: 50px;
  background: #FFFFFF;
  font-size: 14px;
  color: #353535;
}
#order-index .navbar .navbar-item {
  flex: 1;
  width: 0%;
  text-align: center;
}
#order-index .navbar .navbar-item.activity {
  color: #1aad19;
}
#order-index .navbar .navbar-slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width: 4em;
  height: 3px;
  background-color: #1aad19;
  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  transition: transform .3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}
#order-index .seach-box {
  padding-top: 50px;
  margin: 10px;
}
#order-index .seach-box .seach {
  position: relative;
}
#order-index .seach-box .seach icon {
  position: absolute;
  top: 6px;
  left: 10px;
  z-index: 5;
}
#order-index .seach-box .seach input {
  width: 100%;
  height: 30px;
  line-height: 30px;
  background: #FFFFFF;
  border-radius: 10px;
  padding-left: 35px;
  box-sizing: border-box;
}
#order-index .order-box .order-item {
  margin-bottom: 10px;
  background: #FFFFFF;
  font-size: 15px;
}
#order-index .order-box .order-item .item-top .indent {
  line-height: 35px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 15px;
}
#order-index .order-box .order-item .item-top .indent .order-num {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  flex: 1;
  padding-right: 10px;
}
#order-index .order-box .order-item .item-top .indent .order-num span {
  font-weight: bold;
}
#order-index .order-box .order-item .item-top .indent .order-type {
  color: #1aad19;
}
#order-index .order-box .order-item .item-middle .middle-item {
  display: flex;
  padding: 10px 15px;
  color: #959595;
}
#order-index .order-box .order-item .item-middle .middle-item image {
  width: 70px;
  height: 70px;
  border: 1px solid #e5e5e5;
}
#order-index .order-box .order-item .item-middle .middle-item .text {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
  padding-left: 10px;
  overflow: hidden;
}
#order-index .order-box .order-item .item-middle .middle-item .text .names {
  font-size: 17px;
  color: #000000;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  line-height: 22px;
}
#order-index .order-box .order-item .item-middle .middle-item .text .model {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 12px;
}
#order-index .order-box .order-item .item-middle .middle-item .text .model .money {
  color: #ee2b1d;
}
#order-index .order-box .order-item .item-middle .middle-item .text .model .num {
  color: #959595;
}
#order-index .order-box .order-item .all-money {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 6px 15px;
  font-size: 12px;
  color: #959595;
}
#order-index .order-box .order-item .all-money .user i {
  color: #e73237;
  margin-right: 5px;
}
#order-index .order-box .order-item .all-money .num-box sapn {
  color: #ee2b1d;
}
#order-index .order-box .order-item .item-bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px 15px;
}
#order-index .order-box .order-item .item-bottom .guide {
  font-size: 15px;
  color: #959595;
  flex: 1;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
#order-index .order-box .order-item .item-bottom .guide span {
  color: #000000;
}
#order-index .order-box .order-item .item-bottom .buttom-box {
  display: flex;
  align-items: center;
  font-size: 13px;
}
#order-index .order-box .order-item .item-bottom .buttom-box view {
  padding: 5px 15px;
  border-radius: 3px;
  min-width: 72px;
  text-align: center;
}
#order-index .order-box .order-item .item-bottom .buttom-box .code {
  color: #FFFFFF;
  background: #ff0000;
  border: 1px solid #ff0000;
}
#order-index .order-box .order-item .item-bottom .buttom-box .send {
  background: #1aad19;
  color: #FFFFFF;
  border: 1px solid #1aad19;
}
#order-index .order-box .order-item .item-bottom .buttom-box .close {
  background: #f8f8f8;
  border: 1px solid #e4e4e4;
  color: #000;
  margin: 0 10px;
}
#order-index .order-box .order-item .item-bottom .buttom-box .delete {
  color: #1aad19;
  border: 1px solid #1aad19;
}
#order-index .order-box .loadingbox {
  height: 40px;
  line-height: 40px;
  text-align: center;
}
#order-index .order-box .no-list {
  text-align: center;
  padding: 10px 0;
}
